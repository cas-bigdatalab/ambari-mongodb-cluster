{
  "name": "default",
  "description": "default quick links configuration",
  "configuration": {

    "links": [
      {
        "name": "Ops Manager UI",
        "label": "Ops Manager UI",
        "requires_user_name": "false",
        "component_name": "OPS_SERVER",
        "url":"%@://%@:%@/",
        "port":{
          "http_property": "mongodb.opsserver.webapp.address",
          "http_default_port": "8080",
          "https_property": "mongodb.opsserver.webapp.https.address",
          "https_default_port": "8080",
          "regex": "\\w*:(\\d+)",
          "site": "mmsserver"
        }
      }
    ]
  }
}